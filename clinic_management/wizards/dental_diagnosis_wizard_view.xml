<odoo>
    <record id="view_add_diagnosis_wizard_form" model="ir.ui.view">
        <field name="name">add.diagnosis.wizard.form</field>
        <field name="model">dental.diagnosis.wizard</field>
        <field name="arch" type="xml">
            <form string="Add Diagnosis">
                <group>
                    <field name="diagnosis_test"/>
                    <field name="test_date"/>
                    <field name="dentist_id"/>
                    <field name="patient_id"/>
                    <field name="test_result"/>
                </group>
                <footer>
                    <button name="action_add_diagnosis" string="Add Diagnosis" type="object" class="btn-primary"/>
                    <button string="Close" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
