<odoo>
    <record id="view_partner_form_inherit_dental_invoicing" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.dental.invoicing</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_id']" position="after">
                <group string="Dental Clinic Company">
                    <field name="is_insurance_company"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>