<odoo>
    <data noupdate="1">
        <record id="seq_dental_appointment" model="ir.sequence">
            <field name="name">Dental Appointment</field>
            <field name="code">dental.appointment</field>
            <field name="prefix">AP/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_dental_case" model="ir.sequence">
            <field name="name">Dental Case</field>
            <field name="code">dental.case</field>
            <field name="prefix">DC/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_dental_diagnosis" model="ir.sequence">
            <field name="name">Dental Diagnosis</field>
            <field name="code">dental.diagnosis</field>
            <field name="prefix">DI/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_dental_insurance_claim" model="ir.sequence">
            <field name="name">Dental Insurance Claim Sequence</field>
            <field name="code">dental.insurance.claim</field>
            <field name="prefix">DIC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
